isplay_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="WpiszWynik" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="106"/><column index="2" value="106"/><column index="3" value="45"/><column index="4" value="62"/><column index="5" value="62"/><column index="6" value="137"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="WyborLig" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="43"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">CREATE VIEW WpiszWynik AS
SELECT UserA.name || ' ' || UserA.surname AS &quot;gracz A&quot;,
    UserB.name || ' ' || UserB.surname AS &quot;gracz B&quot;,
    playerAscore || ':' || playerBscore AS wynik,
    Queues.ID AS queueID, Matches.ID AS matchID,
    Matches.date AS &quot;data odbycia meczu&quot;
FROM Seasons
JOIN Leagues ON Seasons.leagueID = Leagues.ID
JOIN Queues ON Seasons.ID = Queues.seasonID
JOIN Courts ON Courts.ID = Matches.courtID
JOIN Matches ON Queues.ID = Matches.queueID
JOIN Players AS PlayerA ON Matches.playeraID = PlayerA.ID
JOIN Users AS UserA ON PlayerA.userID = UserA.ID
JOIN Players AS PlayerB ON Matches.playerbID = PlayerB.ID
JOIN Users AS UserB ON PlayerB.userID = UserB.ID</sql><sql name="SQL 2">CREATE VIEW HarmonogramSpotkan AS
SELECT Leagues.discipline AS Dyscyplina, Leagues.name AS &quot;Nazwa ligi&quot;,
    UserA.name || ' ' || UserA.surname AS &quot;gracz A&quot;,
    UserB.name || ' ' || UserB.surname AS &quot;gracz B&quot;,
    playerAscore || ':' || playerBscore AS wynik,
    Courts.name AS boisko, Matches.date AS data
FROM Seasons
JOIN Leagues ON Seasons.leagueID = Leagues.ID
JOIN Queues ON Seasons.ID = Queues.seasonID
JOIN Courts ON Courts.ID = Matches.courtID
JOIN Matches ON Queues.ID = Matches.queueID
JOIN Players AS PlayerA ON Matches.playeraID = PlayerA.ID
JOIN Users AS UserA ON PlayerA.userID = UserA.ID
JOIN Players AS PlayerB ON Matches.playerbID = PlayerB.ID
JOIN Users AS UserB ON PlayerB.userID = UserB.ID</sql><sql name="SQL 3">CREATE VIEW DodajGracza AS
SELECT Users.name AS Imie, Users.surname AS Nazwisko, Players.ID AS PlayerID, Players.leagueID AS LeagueID,
    Players.level AS Poziom, Players.queueID AS QueueID
FROM Players
JOIN Users ON Players.userID = Users.ID</sql><sql name="SQL 4">CREATE VIEW EdycjaLigi AS
SELECT
    Users.name || ' ' || Users.surname AS Gracz,
    Players.level AS Poziom,
	Players.points AS [Wynik poprz. kol.],
    CASE WHEN QueuePayments.paid = 1 THEN 'tak' ELSE 'nie' END AS Opłacono,
    Players.ID AS PlayerID,
    Queues.ID AS QueueID
FROM
    Seasons
INNER JOIN Leagues ON Seasons.leagueID = Leagues.ID
INNER JOIN Queues ON Seasons.ID = Queues.seasonID
INNER JOIN Players ON Queues.ID = Players.queueID
INNER JOIN QueuePayments ON Queues.ID = QueuePayments.queueID AND Players.ID = QueuePayments.playerID
INNER JOIN Users ON Players.userID = Users.ID
WHERE
    Queues.date &gt; CURRENT_TIMESTAMP</sql><sql name="SQL 5">CREATE VIEW HelperView1 AS
SELECT Queues.ID AS QueueID, Queues.date AS QueueDate, Leagues.ID AS LeagueID
FROM Seasons
JOIN Leagues ON Seasons.leagueID = Leagues.ID
JOIN Queues ON Seasons.ID = Queues.seasonID</sql><sql name="SQL 6">CREATE VIEW Ranking AS
SELECT Users.name AS Imie, Users.surname AS Nazwisko,
    Players.level AS Poziom, Players.wonMatches AS &quot;wygrane mecze&quot;,
    Players.wonSets AS &quot;wygrane sety&quot;, Players.ID AS PlayerID,
    Players.leagueID
FROM Players
JOIN Users ON Players.userID = Users.ID</sql><sql name="SQL 7">CREATE VIEW TablicaOgloszen AS
SELECT Advertisements.textBody || CHAR(13) || CHAR(10) || DATETIME(Advertisements.DStamp) || ' - ' ||
    Users.name || ' ' || Users.surname || CHAR(13) || CHAR(10) || ' | REKLAMA|' AS AdvertisementDetails,
    Advertisements.ID
FROM Advertisements
JOIN Users ON Advertisements.userID = Users.ID
ORDER BY Advertisements.DStamp</sql><sql name="SQL 8">CREATE VIEW WyborLig AS
SELECT name
FROM Leagues</sql><sql name="SQL 9">CREATE VIEW EdycjaLigi AS
